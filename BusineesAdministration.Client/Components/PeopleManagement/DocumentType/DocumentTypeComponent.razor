@page "/documenttype"
<h3>Gestión de tipo de documento</h3>
<div>
    <MatButton Raised="true" OnClick="NavigationTo"> Crear tipo de documento</MatButton>
</div>
<div>
    <MatTable Items="@ListDocumentType" class="mat-elevation-z5">
        <MatTableHeader>
            <th>Id</th>
            <th>Nombre del tipo de documento</th>
            <th>Acción</th>
        </MatTableHeader>
        <MatTableRow>
            <td>@context.DocumentTypeId</td>
            <td>@context.DocumentType</td>
            <td><a href="documentType/editar/@context.DocumentTypeId">Edtiar</a> </td>
        </MatTableRow>
    </MatTable>
</div>
